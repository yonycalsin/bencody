{
   "name": "bencody",
   "version": "2.20.11",
   "description": "Find up to date snippets for JavaScript & React use cases",
   "keywords": [
      "gatsby"
   ],
   "license": "MIT",
   "author": "Yony Calsin <helloyonycalsin@gmail.com>",
   "main": "n/a",
   "scripts": {
      "boot": "run-s clean build start",
      "build": "gatsby build",
      "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
      "build:pp": "gatsby build --prefix-paths",
      "clean": "rm -rf public && rm -rf .cache",
      "dev": "run-s clean develop",
      "develop": "gatsby develop",
      "fetch": "sh ./scripts/git-fetch.sh",
      "format": "run-s format:*",
      "format:another": "prettier --write ./**/*.{json,md,mdx}",
      "format:js": "prettier '**/*.{js,jsx,tsx,ts}' --write",
      "format:styles": "prettier '**/*.{css,scss}' --write",
      "lint": "run-s lint:*",
      "lint:js": "eslint --ext .js,.jsx,.tsx,.ts .",
      "lint:md": "remark content/",
      "lint:styles": "stylelint -s scss src/**/*.scss",
      "serve": "gatsby serve",
      "start": "serve -s public",
      "type-check": "tsc",
      "write-good": "write-good $(glob 'content/posts/**/*.md')"
   },
   "remarkConfig": {
      "plugins": [
         "remark-preset-lint-recommended"
      ]
   },
   "dependencies": {
      "babel-plugin-styled-components": "^1.12.0",
      "clsx": "^1.1.1",
      "flexbox-fast": "^1.0.0",
      "gatsby": "^2.26.1",
      "gatsby-image": "^2.10.0",
      "gatsby-plugin-catch-links": "^2.4.0",
      "gatsby-plugin-feed": "^2.7.0",
      "gatsby-plugin-google-analytics": "^2.5.0",
      "gatsby-plugin-lodash": "^3.4.0",
      "gatsby-plugin-manifest": "^2.6.1",
      "gatsby-plugin-nprogress": "^2.4.0",
      "gatsby-plugin-offline": "^3.4.0",
      "gatsby-plugin-react-helmet": "^3.4.0",
      "gatsby-plugin-sharp": "^2.8.0",
      "gatsby-plugin-sitemap": "^2.11.0",
      "gatsby-plugin-styled-components": "^3.7.0",
      "gatsby-plugin-twitter": "^2.7.0",
      "gatsby-remark-autolink-headers": "^2.10.0",
      "gatsby-remark-copy-linked-files": "^2.4.0",
      "gatsby-remark-images": "^3.5.1",
      "gatsby-remark-prismjs": "^3.11.0",
      "gatsby-remark-relative-images": "^2.0.2",
      "gatsby-remark-responsive-iframe": "^2.5.0",
      "gatsby-source-filesystem": "^2.5.0",
      "gatsby-transformer-remark": "^2.10.0",
      "gatsby-transformer-sharp": "^2.6.0",
      "lodash": "^4.17.20",
      "moment": "^2.29.1",
      "npm-run-all": "^4.1.5",
      "prismjs": "^1.23.0",
      "react": "^17.0.1",
      "react-disqus-comments": "^1.4.0",
      "react-dom": "^17.0.1",
      "react-helmet": "^6.1.0",
      "react-share": "^4.3.1",
      "react-twitter-widgets": "^1.9.5",
      "scroll-style": "^1.0.0",
      "styled-components": "^5.2.1",
      "url-join": "^4.0.0"
   },
   "devDependencies": {
      "@types/node": "^14.14.14",
      "@types/react": "^17.0.0",
      "@types/react-dom": "^17.0.0",
      "@types/styled-components": "^5.1.7",
      "@typescript-eslint/eslint-plugin": "^4.10.0",
      "@typescript-eslint/parser": "^4.12.0",
      "cli-glob": "^0.1.0",
      "eslint": "^7.16.0",
      "eslint-config-airbnb": "^18.2.0",
      "eslint-config-prettier": "^7.1.0",
      "eslint-plugin-import": "^2.22.0",
      "eslint-plugin-jsx-a11y": "^6.3.1",
      "eslint-plugin-prettier": "^3.3.0",
      "eslint-plugin-react": "^7.21.5",
      "eslint-plugin-react-hooks": "^4.2.0",
      "gatsby-plugin-root-import": "^2.0.5",
      "gatsby-plugin-sass": "^2.8.0",
      "gh-pages": "^3.1.0",
      "husky": "^4.3.6",
      "lint-staged": "^10.5.3",
      "node-sass": "^4.14.1",
      "prettier": "^2.1.2",
      "prettier-plugin-organize-imports": "^1.1.1",
      "prettier-plugin-packagejson": "^2.2.9",
      "remark-cli": "^9.0.0",
      "remark-preset-lint-recommended": "^5.0.0",
      "stylelint": "^13.8.0",
      "stylelint-config-prettier": "^8.0.2",
      "stylelint-config-standard": "^20.0.0",
      "stylelint-prettier": "^1.1.2",
      "typescript": "^4.1.3",
      "write-good": "^1.0.3"
   }
}
