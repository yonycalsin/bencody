{
   "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "esnext",
      "jsx": "preserve",
      "lib": ["dom", "esnext"],
      "strict": true,
      "noEmit": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "noUnusedLocals": false,
      "noImplicitAny": false,
      "baseUrl": ".",
      "allowJs": true
   },
   "exclude": ["node_modules", "public", ".cache"]
}
